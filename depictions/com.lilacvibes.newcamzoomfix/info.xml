<package>
    <id>com.lilacvibes.newcamzoomfix</id>
    <name>NewCamZoomFix</name>
    <version>1.0</version>
    <compatibility>
        <firmware>
            <miniOS>13.0</miniOS>
            <otherVersions>unsupported</otherVersions>
            <!--
             for otherVersions, you can put either unsupported or unconfirmed
             -->
        </firmware>
    </compatibility>
    <dependencies>
        <package>NewCam</package>
    </dependencies>
    <descriptionlist>
        <description><![CDATA[
		Fixes camera zoom when using NewCam<br/>
		Requires NewCam<br/> 
		Compatible with iOS 13<br/>
		No options to configure
		]]></description>
    </descriptionlist>
    <screenshots></screenshots>
    <changelog>
        <change>Initial release</change>
    </changelog>
    <links></links>
</package>
